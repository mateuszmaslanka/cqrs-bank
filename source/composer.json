{
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "require": {
        "php": "^8.4",
        "psr/cache": "^3.0",
        "symfony/console": "^7.2",
        "symfony/flex": "^2.4",
        "symfony/framework-bundle": "^7.2",
        "symfony/messenger": "^7.2",
        "symfony/runtime": "^7.2",
        "symfony/twig-bundle": "^7.2",
        "symfony/yaml": "^7.2"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
